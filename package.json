{
  "name": "md-fileserver",
  "version": "1.4.0-0",
  "description": "Locally view markdown files in a browser",
  "keywords": [
    "fileserver",
    "markdown",
    "server"
  ],
  "homepage": "https://github.com/commenthol/md-fileserver",
  "bugs": {
    "url": "http://github.com/commenthol/md-fileserver/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/commenthol/md-fileserver.git"
  },
  "license": "MIT",
  "author": "commenthol",
  "main": "lib/index.js",
  "bin": {
    "mdstart": "./bin/cli.js"
  },
  "man": "./man/mdstart.1",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "all": "npm-run-all clean lint build test",
    "build": "webpack -p",
    "clean": "rimraf assets/fonts assets/index* assets/*.hot-update.js* .nyc_output coverage *.tgz",
    "coverage": "nyc -r html -r text npm test",
    "dev": "webpack -w",
    "postinstall": "semver-dedupe katex",
    "lint": "eslint \"**/*.js\"",
    "man": "ronn -r man/mdstart.md && man man/mdstart.1 > man/mdstart.txt",
    "prepublishOnly": "npm run all",
    "readme": "markedpp --githubid -i README.md -o README.md",
    "test": "mocha",
    "test:md": "node bin/cli.js test/cheatsheet.md"
  },
  "dependencies": {
    "asyncc": "^2.0.3",
    "body-parser": "^1.18.3",
    "cookie": "^0.3.1",
    "express": "^4.16.4",
    "highlight.js": "^9.15.6",
    "katex": "^0.10.1",
    "lodash.template": "^4.4.0",
    "markdown-it": "^8.4.2",
    "markdown-it-anchor": "^5.0.2",
    "markdown-it-emoji": "^1.4.0",
    "markdown-it-footnote": "^3.0.1",
    "markdown-it-highlightjs": "^3.0.0",
    "markdown-it-katex": "^2.0.3",
    "markdown-it-task-lists": "^2.1.1",
    "markedpp": "^1.0.0",
    "semver-dedupe": "^1.0.0",
    "serve-index": "^1.9.1",
    "serve-static": "^1.13.2",
    "ws": "^6.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/register": "^7.4.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^4.0.0",
    "file-loader": "^3.0.1",
    "mini-css-extract-plugin": "^0.5.0",
    "mocha": "^6.0.2",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "style-loader": "^0.23.1",
    "supertest": "^4.0.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  },
  "engine": {
    "node": ">=8.0.0"
  }
}
