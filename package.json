{
  "name": "md-fileserver",
  "version": "1.3.1",
  "description": "Locally view markdown files in a browser",
  "keywords": [
    "fileserver",
    "markdown",
    "server"
  ],
  "homepage": "https://github.com/commenthol/md-fileserver",
  "bugs": {
    "url": "http://github.com/commenthol/md-fileserver/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/commenthol/md-fileserver.git"
  },
  "license": "MIT",
  "author": "commenthol",
  "main": "lib/index.js",
  "bin": {
    "mdopen": "./bin/mdopen.js",
    "mdstart": "./bin/mdstart.js"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "clean": "rimraf .nyc_output coverage node_modules *.tgz",
    "copy": "node scripts/prebuild.js",
    "coverage": "nyc -r html -r text npm test",
    "lint": "eslint \"**/*.js\"",
    "prepare": "npm-run-all lint copy test",
    "readme": "markedpp --githubid -i README.md -o README.md",
    "test": "mocha",
    "test:md": "node bin/mdopen.js test/cheatsheet.md"
  },
  "dependencies": {
    "asyncc": "^2.0.3",
    "express": "^4.16.2",
    "highlight.js": "^9.12.0",
    "lodash": "^4.17.5",
    "markdown-it": "^8.4.1",
    "markdown-it-anchor": "^5.0.2",
    "markdown-it-emoji": "^1.4.0",
    "markdown-it-footnote": "^3.0.1",
    "markdown-it-katex": "^2.0.3",
    "markdown-it-task-lists": "^2.1.0",
    "markedpp": "^0.3.10",
    "serve-index": "^1.9.1",
    "serve-static": "^1.13.2",
    "ws": "^6.0.0"
  },
  "devDependencies": {
    "eslint": "^5.10.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^13.1.0",
    "shelljs": "^0.8.3",
    "supertest": "^3.3.0"
  },
  "engine": {
    "node": ">=6.0.0"
  }
}
