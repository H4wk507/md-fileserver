{
  "name": "md-fileserver",
  "version": "1.7.1",
  "description": "Locally view markdown files in a browser",
  "keywords": [
    "fileserver",
    "markdown",
    "server"
  ],
  "homepage": "https://github.com/commenthol/md-fileserver",
  "bugs": {
    "url": "http://github.com/commenthol/md-fileserver/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/commenthol/md-fileserver.git"
  },
  "license": "MIT",
  "author": "commenthol",
  "main": "lib/index.js",
  "bin": {
    "mdstart": "bin/mdstart.js"
  },
  "man": "./man/mdstart.1",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "all": "npm-run-all clean lint build test",
    "build": "webpack --mode production",
    "clean": "rimraf assets/fonts assets/index* assets/*.hot-update.js* .nyc_output coverage *.tgz",
    "coverage": "nyc -r html -r text npm test",
    "dev": "webpack -w",
    "__postinstall": "semver-dedupe katex@^0.6 markdown-it@^8 highlight.js@^9",
    "lint": "eslint \"**/*.js\"",
    "man": "ronn -r man/mdstart.md && man man/mdstart.1 > man/mdstart.txt",
    "prepublishOnly": "npm run all",
    "readme": "markedpp --githubid -i README.md -o README.md",
    "test": "mocha",
    "test:md": "bin/mdstart.js test/cheatsheet.md",
    "test:cnfl": "bin/mdstart.js test/cnfl.md"
  },
  "mocha": {
    "exit": true
  },
  "dependencies": {
    "@commenthol/markdown-it-katex": "^2.0.6",
    "asyncc": "^2.0.4",
    "body-parser": "^1.19.0",
    "confluencer": "^1.4.2",
    "cookie": "^0.4.1",
    "express": "^4.17.1",
    "highlight.js": "^10.7.2",
    "katex": "^0.13.11",
    "lodash.template": "^4.5.0",
    "markdown-it": "^12.0.6",
    "markdown-it-anchor": "^7.1.0",
    "markdown-it-emoji": "^2.0.0",
    "markdown-it-footnote": "^3.0.2",
    "markdown-it-highlightjs": "^3.5.0",
    "markdown-it-multimd-table": "^4.1.0",
    "markdown-it-task-lists": "^2.1.1",
    "markedpp": "^1.0.3",
    "semver-dedupe": "^1.1.2",
    "serve-index": "^1.9.1",
    "serve-static": "^1.14.1",
    "ws": "^7.4.5"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-transform-react-jsx": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/register": "^7.13.16",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.4",
    "eslint": "^7.26.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.23.2",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^1.6.0",
    "mocha": "^8.4.0",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "style-loader": "^2.0.0",
    "supertest": "^6.1.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.37.0",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2"
  },
  "engine": {
    "node": ">=8.0.0"
  }
}
